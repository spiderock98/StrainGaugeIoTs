        <form id="formCal" action="/calendar" method="POST">
            <div class="input-group input-group-lg">
                <div class="input-group-prepend">
                    <input class="btn btn-outline-success" type="submit" name="showLog" value="View Log">
                </div>
                <input class="form-control" type="date" id="qrLog">
                <div class="input-group-prepend">
                    <button class="btn btn-outline-info" id="btnToday">
                        <i class="far fa-calendar-alt"> Today</i>
                    </button>
                </div>
            </div>
            <div>
                <input style="display: inline;" class="btn btn-primary btn-lg btn-block" type="submit" name="download"
                    value="Download .xlsx">
            </div>
        </form>


        <div id="dashboard" style="margin: 50px;">
            <div id="linechart"></div>
            <div id="filter-range"></div>
        </div>

