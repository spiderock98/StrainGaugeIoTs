    <main>
        <div class="input-group input-group-lg">
            <div class="input-group-prepend">
                <label class="input-group-text" for="inputGroupSelect01"><i class="fas fa-map-marked-alt">
                        Address</i></label>
            </div>
            <select required class="custom-select" id="combobox" name="locatCombobox" form="formCal">
